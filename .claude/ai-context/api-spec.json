{
  "service": "oms-core",
  "baseUrl": "http://localhost:8081",
  "version": "0.0.1-SNAPSHOT",
  "endpoints": {
    "health": {
      "description": "헬스체크",
      "apis": [
        {
          "method": "GET",
          "path": "/health",
          "description": "서버 상태 확인",
          "status": "IMPLEMENTED",
          "response": {
            "status": "String (UP)",
            "service": "String (oms-core)",
            "timestamp": "String (ISO DateTime)"
          }
        },
        {
          "method": "GET",
          "path": "/server/properties",
          "description": "서버 버전 정보",
          "status": "IMPLEMENTED",
          "response": {
            "version": "String (0.0.1)",
            "service": "String (oms-core)"
          }
        }
      ]
    }
  }
}
